    box-shadow: 2px 2px 5px rgba(0, 0, 0, 0.3); /* Shadow */
}

QPushButton:hover {
    background-color: #2980b9; /* Darker blue when hovered */
    border-color: #1c6391; /* Darker border when hovered */
