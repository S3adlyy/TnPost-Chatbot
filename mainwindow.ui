}

QPushButton:pressed {
    background-color: #1c6391; /* Even darker blue when pressed */
    border-color: #14506d; /* Even darker border when pressed */
    box-shadow: 2px 2px 5px rgba(0, 0, 0, 0.3); /* Slightly weaker shadow when pressed */
}

